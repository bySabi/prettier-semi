{
  "name": "prettier-semi",
  "version": "1.0.0",
  "description": "prettier -> semistandard --fix -> Formatted Code",
  "keywords": [
    "prettier",
    "eslint",
    "prettier-eslint",
    "linter",
    "standard",
    "standard-engine",
    "semistandard"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "lint": "semistandard --verbose | snazzy",
    "test": "npm run lint",
    "format": "prettier-semi --no-bracket-spacing --write '**/*.js'"
  },
  "dependencies": {
    "prettier-eslint": "*",
    "semistandard": "*",
    "semistandard-prettier-eslint": "1.x.x"
  },
  "devDependencies": {
    "snazzy": "*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bySabi/prettier-semi.git"
  },
  "bugs": {
    "url": "https://github.com/bySabi/prettier-semi/issues"
  },
  "homepage": "https://github.com/bySabi/prettier-semi#readme",
  "author": "bySabi Files",
  "license": "MIT"
}
